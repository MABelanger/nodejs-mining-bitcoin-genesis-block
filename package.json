  "scripts": {
    "start": "npm run test:watch",
    "check-coverage": "nyc check-coverage",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist src/lib",
    "test": "mocha src/lib/**/*.test.js --compilers js:babel-register",
    "test:watch": "npm t -- -w",
    "nodemon:babel": "./node_modules/.bin/nodemon --watch src --exec ./node_modules/.bin/babel-node src/lib/index.js",
    "eslint:src": "eslint --ignore-pattern !**/*.{test,mock}.js src/",
    "eslint:watch": "esw --color --ignore-pattern !**/*.{test,mock}.js src/ -w",
    "eslint:fix": "eslint --fix src/"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "3.15.0",
    "eslint-config-standard": "7.0.0-beta.0",
    "eslint-plugin-promise": "3.4.1",
    "eslint-plugin-standard": "2.0.1",
    "eslint-watch": "2.1.14",
    "http-proxy": "1.16.2",
    "json-loader": "0.5.4",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nyc": "10.1.2",
    "rimraf": "2.5.4",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "aes-js": "3.0.0",
    "babel-cli": "^6.24.1",
    "binascii": "0.0.1",
    "body-parser": "1.17.1",
    "chai": "^3.5.0",
    "crypto-js": "3.1.9-1",
    "dnsd": "^0.9.6",
    "expect": "^1.20.2",
    "express": "4.15.2",
    "http": "0.0.0",
    "https": "1.0.0",
    "jssha": "2.2.0",
    "node-forge": "^0.7.1",
    "nodemon": "^1.11.0",
    "request": "2.81.0",
    "xml2js": "^0.4.17"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
